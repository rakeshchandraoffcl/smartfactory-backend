{
  "name": "techsquarelse-backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/server.js",
  "nodemonConfig": {
    "ignore": [
      "uploads/*"
    ]
  },
  "scripts": {
    "dev": "nodemon src/server.js",
    "filepath": "set NODE_ENV=filepath&& nodemon src/server.js",
    "pm2": "pm2 start ecosystem.config.json",
    "pm2:local": "pm2 start ecosystem.config.json --env local",
    "pm2:dev": "PM2_HOME=/etc/pm2daemon pm2 start ecosystem.config.json --env development",
    "pm2:dev-restart": "PM2_HOME=/etc/pm2daemon pm2 restart ecosystem.config.json --env development",
    "pm2:prd": "PM2_HOME=/etc/pm2daemon pm2 start ecosystem.config.json --env production",
    "pm2:prd-restart": "PM2_HOME=/etc/pm2daemon pm2 restart ecosystem.config.json --env production",
    "pm2:reload": "PM2_HOME=/etc/pm2daemon pm2 reload all",
    "pm2:log": "PM2_HOME=/etc/pm2daemon pm2 logs --lines 1000",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aes256": "^1.1.0",
