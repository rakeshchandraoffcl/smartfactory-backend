    "celebrate": "^15.0.1",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "crypto-js": "^4.1.1",
    "cryptr": "^6.0.3",
    "dayjs": "^1.11.5",
    "dayjs-plugin-utc": "^0.1.2",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "express-bearer-token": "^2.4.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "i18n": "^0.14.1",
    "json_": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "libxmljs": "^0.19.10",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
